// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/prisma-generated"
}

datasource db {
  provider = "sqlite"
  url      = "file:dev.db"
}

model Counter {
  id    Int @id @default(1)
  value Int @default(0)
}
